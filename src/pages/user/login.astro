---
import LoginContent from "~/components/LoginContent.svelte";
import Layout from "~/layouts/Layout.astro";
import { isLoggedIn } from "~/lib/SharedVerification";

const loggedIn = await isLoggedIn(Astro);

if (loggedIn) {
	return Astro.redirect("/user")
}

---

<Layout title="Login | CyclersHub">
	<LoginContent client:load></LoginContent>
</Layout>