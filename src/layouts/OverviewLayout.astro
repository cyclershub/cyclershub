---
import Layout from "./Layout.astro";

export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
	<div class="bg-stone-200 py-16 px-8 lg:px-16 text-center">
		<slot name="head"></slot>
	</div>
	<div class="bg-stone-100 py-16 px-8 lg:px-16 text-center">
		<slot></slot>
	</div>
</Layout>
